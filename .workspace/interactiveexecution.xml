<?xml version="1.0" encoding="utf-8"?>
<INTERACTIVE-EXECUTION-SETTINGS xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<INTERACTIVE-EXECUTION xsi:type="interactiveExecution">
		<COMMON-SHORTCUTS>
			<ELEMENT dkey="HK_00_EVAL_NONE">
				<DVALUE xsi:type="shortcut">
					<KEY-BINDING xsi:type="string">Ctrl+1</KEY-BINDING>
					<DESCRIPTION xsi:type="string">Evaluate with NONE</DESCRIPTION>
				</DVALUE>
			</ELEMENT>
			<ELEMENT dkey="HK_01_EVAL_SUCCESS">
				<DVALUE xsi:type="shortcut">
					<KEY-BINDING xsi:type="string">Ctrl+2</KEY-BINDING>
					<DESCRIPTION xsi:type="string">Evaluate with SUCCESS</DESCRIPTION>
				</DVALUE>
			</ELEMENT>
			<ELEMENT dkey="HK_02_EVAL_INCONCLUSIVE">
				<DVALUE xsi:type="shortcut">
					<KEY-BINDING xsi:type="string">Ctrl+3</KEY-BINDING>
					<DESCRIPTION xsi:type="string">Evaluate with INCONCLUSIVE</DESCRIPTION>
				</DVALUE>
			</ELEMENT>
			<ELEMENT dkey="HK_03_EVAL_FAILED">
				<DVALUE xsi:type="shortcut">
					<KEY-BINDING xsi:type="string">Ctrl+4</KEY-BINDING>
					<DESCRIPTION xsi:type="string">Evaluate with FAILED</DESCRIPTION>
				</DVALUE>
			</ELEMENT>
			<ELEMENT dkey="HK_04_EVAL_ERROR">
				<DVALUE xsi:type="shortcut">
					<KEY-BINDING xsi:type="string">Ctrl+5</KEY-BINDING>
					<DESCRIPTION xsi:type="string">Evaluate with ERROR</DESCRIPTION>
				</DVALUE>
			</ELEMENT>
			<ELEMENT dkey="HK_10_SKIP_COMPOMENT">
				<DVALUE xsi:type="shortcut">
					<KEY-BINDING xsi:type="string">Space</KEY-BINDING>
					<DESCRIPTION xsi:type="string">Skip project step</DESCRIPTION>
				</DVALUE>
			</ELEMENT>
			<ELEMENT dkey="HK_11_ABORT_EXECUTION">
				<DVALUE xsi:type="shortcut">
					<KEY-BINDING xsi:type="string">Esc</KEY-BINDING>
					<DESCRIPTION xsi:type="string">Abort test case</DESCRIPTION>
				</DVALUE>
			</ELEMENT>
			<ELEMENT dkey="HK_12_DELAYED_ABORT">
				<DVALUE xsi:type="shortcut">
					<KEY-BINDING xsi:type="string">Ctrl+C</KEY-BINDING>
					<DESCRIPTION xsi:type="string">Cancel after current project step</DESCRIPTION>
				</DVALUE>
			</ELEMENT>
			<ELEMENT dkey="HK_13_PAUSE">
				<DVALUE xsi:type="shortcut">
					<KEY-BINDING xsi:type="string">Ctrl+P</KEY-BINDING>
					<DESCRIPTION xsi:type="string">Pause test case</DESCRIPTION>
				</DVALUE>
			</ELEMENT>
			<ELEMENT dkey="HK_14_CONTINUE">
				<DVALUE xsi:type="shortcut">
					<KEY-BINDING xsi:type="string">Ctrl+F</KEY-BINDING>
					<DESCRIPTION xsi:type="string">Continue test case</DESCRIPTION>
				</DVALUE>
			</ELEMENT>
			<ELEMENT dkey="HK_20_SWITCH_TESTCASE_LANGUAGE">
				<DVALUE xsi:type="shortcut">
					<KEY-BINDING xsi:type="string">Alt+Ctrl+L</KEY-BINDING>
					<DESCRIPTION xsi:type="string">Switch test case language</DESCRIPTION>
				</DVALUE>
			</ELEMENT>
			<ELEMENT dkey="HK_30_REPEAT_ANNOUNCEMENT">
				<DVALUE xsi:type="shortcut">
					<KEY-BINDING xsi:type="string">Ctrl+0</KEY-BINDING>
					<DESCRIPTION xsi:type="string">Repeat last message</DESCRIPTION>
				</DVALUE>
			</ELEMENT>
			<ELEMENT dkey="HK_40_SET_COMMENT">
				<DVALUE xsi:type="shortcut">
					<KEY-BINDING xsi:type="string">Ctrl+N</KEY-BINDING>
					<DESCRIPTION xsi:type="string">Add comment</DESCRIPTION>
				</DVALUE>
			</ELEMENT>
		</COMMON-SHORTCUTS>
	</INTERACTIVE-EXECUTION>
</INTERACTIVE-EXECUTION-SETTINGS>
